<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="562.0" prefWidth="868.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.genshinwishsimapp.StandardBanner">
   <children>
      <ImageView fitHeight="718.0" fitWidth="1262.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/standardBanner.jpg" />
         </image>
      </ImageView>
      <Button fx:id="singleSummon" layoutX="755.0" layoutY="634.0" mnemonicParsing="false" onAction="#singleSummon" opacity="0.0" prefHeight="61.0" prefWidth="232.0" text="1x" />
      <Button fx:id="multiSummon" layoutX="1002.0" layoutY="634.0" mnemonicParsing="false" onAction="#multiSummon" opacity="0.0" prefHeight="61.0" prefWidth="232.0" text="10x" />
      <Label fx:id="PrimoGemsAmount" layoutX="821.0" layoutY="589.0" text="Primogems:" textFill="#f5f2f2">
         <font>
            <Font name="Arial" size="31.0" />
         </font>
      </Label>
      <Button fx:id="WeaponBannerButton" layoutX="32.0" layoutY="630.0" mnemonicParsing="false" onAction="#switchToWeaponBanner" prefHeight="61.0" prefWidth="385.0" text="Weapon Banner" />
      <Label fx:id="SummonsAvailable" layoutX="38.0" layoutY="584.0" text="Summons:" textFill="#fcfafa">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
